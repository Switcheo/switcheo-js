{
  "name": "switcheo-js",
  "version": "1.1.25",
  "description": "Switcheo Library",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/Switcheo/switcheo-js",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "info": "npm-scripts-info",
    "prepare": "yarn run build",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "tslint --project .",
    "test:unit": "nyc --silent ava",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"build:module -- -w\" \"test:unit -- --watch\"",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && yarn install",
    "clean": "trash build test",
    "all": "run-s reset test cov:check doc:html",
    "prepare-release": "run-s all version doc:publish"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@cityofzion/neon-core": "^4.0.2",
    "@ledgerhq/hw-app-eth": "4.21.0",
    "@ledgerhq/hw-transport": "4.21.0",
    "@ledgerhq/hw-transport-u2f": "4.21.0",
    "@types/bip39": "^2.4.1",
    "@types/bitcoinjs-lib": "^3.2.2",
    "@types/hdkey": "^0.7.0",
    "@types/long": "^4.0.0",
    "@types/node": "^10.12.18",
    "axios": "^0.18.0",
    "bignumber.js": "^7.2.1",
    "bip39": "^2.5.0",
    "bip44-constants": "^7.0.0",
    "bitcoinjs-message": "^2.0.0",
    "eosjs": "16.0.9",
    "eosjs2": "npm:eosjs@20.0.0-beta3",
    "ethereumjs-tx": "^1.3.7",
    "hdkey": "^1.1.0",
    "humps": "^2.0.1",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.10",
    "ontology-ts-sdk": "^1.0.10",
    "qtumjs-wallet": "^0.2.1",
    "scatterjs-core": "^2.7.7",
    "scatterjs-plugin-eosjs": "^1.5.0",
    "scatterjs-plugin-eosjs2": "^1.5.0",
    "trezor-connect": "^6.0.2",
    "web3": "0.20.6"
  },
  "devDependencies": {
    "@babel/runtime": "^7.3.1",
    "@types/bn.js": "^4.11.1",
    "@types/ethereumjs-tx": "^1.0.0",
    "@types/humps": "^1.1.2",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/ledgerhq__hw-transport": "^4.21.0",
    "@types/ledgerhq__hw-transport-u2f": "^4.21.0",
    "@types/loglevel": "^1.5.3",
    "ava": "^0.25.0",
    "codecov": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "ethereum-types": "^1.1.2",
    "gh-pages": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "opn-cli": "^3.1.0",
    "standard-version": "^4.4.0",
    "trash-cli": "^1.4.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-immutable": "^4.5.4",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1",
    "typescript-eslint-parser": "^17.0.1"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "types": "src/index.d.ts"
}
